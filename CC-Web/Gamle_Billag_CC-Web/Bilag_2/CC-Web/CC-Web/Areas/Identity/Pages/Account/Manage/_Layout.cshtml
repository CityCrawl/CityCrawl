@{
    if (ViewData.TryGetValue("ParentLayout", out var parentLayout))
    {
        Layout = (string)parentLayout;
    }
    else
    {
        Layout = "/Areas/Identity/Pages/_Layout.cshtml";
    }
}

<img src="~/Images/CCLogo.png" class="generel_style_image" />
<div>
    <hr />
    <div class="row">
        <div class="col-md-6">
            <h1>Virksomhedsprofil</h1>
            <p>Virksomhedsnavn:</p>
            <p>CVR-nummer: </p>
            <p>Kontaktperson</p>
            <p>E-mail:</p>
            <p>Virksomhedsbeskrivelse: </p>

            <textarea id="textareaV" style="width:350px;height:200px"></textarea>
            <br />
            <input type="button" class="PinkBtn" value="Gem" style="margin-top:5%" />
        </div>
        <!--<div class="col-md-6">
            @RenderBody()
        </div>-->
    </div>
</div>

@section Scripts {
    @RenderSection("Scripts", required: false)
}
